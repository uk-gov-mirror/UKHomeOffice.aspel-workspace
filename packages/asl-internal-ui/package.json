{
  "name": "asl-internal-ui",
  "version": "3.7.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "npm-run-all --parallel watch:client watch:server watch:css",
    "watch:client": "npm run build:js -- --watch",
    "watch:server": "nodemon -r dotenv/config -e js,jsx,json -w",
    "watch:css": "npm run build:css -- --watch",
    "test": "npm run test:lint && npm run test:deduped",
    "test:audit": "audit-ci --high --skip-dev --config .auditrc.json",
    "test:lint": "eslint . --ext jsx,js",
    "test:deduped": "./tests/dedupe.sh",
    "build": "npm run build:js && npm run build:css",
    "build:js": "node --max_old_space_size=4096 ../../node_modules/.bin/webpack",
    "prebuild:css": "mkdir -p ./public/css",
    "build:css": "npm-sass ./assets/sass/app.scss > ./public/css/app.css",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/asl-internal-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/asl-internal-ui/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl-internal-ui#readme",
  "dependencies": {
    "@asl/pages": "file:../asl-pages",
    "@asl/projects": "file:../asl-projects",
    "@asl/service": "file:../asl-service",
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@ukhomeoffice/asl-components": "file:../asl-components",
    "@ukhomeoffice/asl-constants": "file:../asl-constants",
    "@ukhomeoffice/react-components": "^1.6.4",
    "accessible-autocomplete": "^3.0.1",
    "audit-ci": "^7.1.0",
    "aws-sdk": "^2.1354.0",
    "babel-loader": "^9.1.3",
    "backpack-transpiled": "0.0.23",
    "body-parser": "^1.20.1",
    "classnames": "^2.3.1",
    "css-loader": "^5.0.0",
    "csv-stringify": "^5.3.6",
    "date-fns": "^3.6.0",
    "docx": "5.0.0-rc4",
    "express": "^4.18.2",
    "filenamify": "^4.1.0",
    "lodash": "^4.17.19",
    "moment": "^2.29.3",
    "mustache": "^2.3.2",
    "ndjson": "^1.5.0",
    "r2": "^2.0.1",
    "react": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "sass": "^1.50.1",
    "style-loader": "^2.0.0",
    "through2": "^3.0.1",
    "uuid": "^8.3.2",
    "uuid-validate": "0.0.3"
  },
  "devDependencies": {
    "@ukhomeoffice/eslint-config-asl": "^3.0.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "dotenv": "^17.2.3",
    "eslint": "^8.25.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "npm-sass": "^3.1.0",
    "webpack": "^5.82.1",
    "webpack-cli": "^4.9.2"
  },
  "overrides": {
    "node-fetch": "^2.6.11",
    "d3-color": "^3.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "8.1.3"
  }
}
